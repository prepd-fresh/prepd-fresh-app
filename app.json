{
  "expo": {
    "name": "Prep'd Fresh",
    "slug": "prepdfresh-react-app",
    "privacy": "public",
    "sdkVersion": "35.0.0",
    "platforms": ["ios", "android", "web"],
    "version": "2.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "android": {
      "package": "com.prepdfresh.prepdfresh"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.prepdfresh.prepdfresh"
    },
    "description": "Hamilton residents order Prep'd Fresh meals through the official Prep'd Fresh app! Spend less time cooking & more time liersiing Healthy, ready-to-eat meals. Free weekly delivery."
  }
}
